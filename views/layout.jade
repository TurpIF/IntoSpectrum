!!!
html
  head
    title=title
    link(type='text/css' rel='stylesheet' href='/bootstrap/css/bootstrap.min.css')
    link(type='text/css' rel='stylesheet' href='/css/base.css')
    script(type='text/javascript' src='/bootstrap/js/bootstrap.min.js')
    script(type='text/javascript' src='/js/audio.js')
    script(type='text/javascript' src='/js/base.js')
  body
    block toolbar
      nav(role='navigation').navbar.navbar-default
        div.navbar-header
          button(type='button' data-toggle='collapse' data-target='.navbar-ex1-collapse').navbar-toggle
            span.sr-only
              | Toggle navigation
            span.icon-bar
            span.icon-bar
            span.icon-bar
          a(href='#').navbar-brand #{brand}
        div.collapse.navbar-collapse.navbar-ex1-collapse
          ul.nav.navbar-nav
            - var obj = { 'home': 'Home', 'about': 'About' }
              - each val, key in obj
                - if (title == key || title == val)
                  li.active
                    a(href='#{key}') #{val}
                - else
                  li
                    a(href='#{key}') #{val}
    block content
